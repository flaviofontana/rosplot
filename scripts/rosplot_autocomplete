#
# Autocomplete for rosplot
#

_rosplot()
{
	local cur prev opts
	COMPREPLY=()
	cur="${COMP_WORDS[COMP_CWORD]}"
	prev="${COMP_WORDS[COMP_CWORD-1]}"
	pkg_path=`rospack find rosplot 2> /dev/null`
	opts=`${pkg_path}/scripts/rosplot_autocomplete_helper ${cur} 2> /dev/null`

	COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
	return 0
}

complete -o filenames -o nospace -F _rosplot  rosplot
